<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hsdj"
    android:targetSandboxVersion="2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="30" />
    <!-- 添加网络权限 -->
    <uses-permission android:name="android.permission.INTERNET" /> <!-- 添加读写权限 -->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="com.example.hsdj.MyApplication"
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.AppCompat.NoActionBar"
        android:usesCleartextTraffic="true" >
        <activity android:name="djjy.stlx.GradeActivity" />
        <activity android:name="djjy.stlx.xtlx.PracticeActivity" />
        <activity android:name="djjy.stlx.ModelSelectionActivity" />
        <activity android:name="djjy.filemange.FileActivity" />
        <activity android:name="djjy.stlx.ExercisesInfoActivity" />
        <activity android:name="login.LoginActivity" />
        <activity android:name="djjy.spkc.VideoInfoActivity" />
        <activity android:name="djjy.stlx.ExercisesActivity" />
        <activity
            android:name="djjy.spkc.VideoActivity"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:screenOrientation="portrait" />
        <activity android:name="person.PerInfoActivity" /> <!-- parentActivityName告诉父类，方便返回（在安卓上点击返回键或左滑即可） -->
        <activity android:name="djjy.djzx.ConsInfoActivity" />
        <activity
            android:name="djjy.djzx.ConsActivity"
            android:parentActivityName="com.example.hsdj.MainActivity" />
        <activity
            android:name="orgmap.OrgActivity"
            android:parentActivityName="com.example.hsdj.MainActivity" />
        <activity
            android:name="person.PersonActivity"
            android:parentActivityName="com.example.hsdj.MainActivity" />
        <activity android:name="com.example.hsdj.NewActivity" />
        <activity android:name="com.example.hsdj.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>